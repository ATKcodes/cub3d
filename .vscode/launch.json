	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "/nfs/homes/amaso/Desktop/cub3d2/cub3D",
			//"program": "/nfs/homes/sfarina/Desktop/cub3d/cub3D",
			"args": ["maps/map.cub"],
			"stopAtEntry": false,
			"cwd": "/nfs/homes/amaso/Desktop/cub3d2",
			//"cwd": "/nfs/homes/sfarina/Desktop/cub3d",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
}